<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="332.0" prefWidth="646.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.demo.controllers.Profile.PaymentMethod.GCashEditController">
    <!-- Credit/Debit Card Fields -->
    <VBox layoutX="84.0" layoutY="78.0" prefHeight="240.0" prefWidth="478.0" spacing="10.0">
      <Label style="-fx-font-weight: bold;" text="GCash Account Name:" textFill="#2e2929">
         <VBox.margin>
            <Insets bottom="-10.0" />
         </VBox.margin>
      </Label>
        <TextField fx:id="gcashAccountField" prefHeight="40.0" prefWidth="326.0" promptText="GCash Account Name" />
      <Label style="-fx-font-weight: bold;" text="GCash Phone Number:" textFill="#2e2929">
         <VBox.margin>
            <Insets bottom="-10.0" />
         </VBox.margin>
      </Label>
        <TextField fx:id="gcashPhoneField" prefHeight="40.0" prefWidth="326.0" promptText="GCash Phone Number" />
      <HBox prefHeight="46.0" prefWidth="474.0">
         <children>
            <Button fx:id="cancelButton" onAction="#onCancel" prefHeight="40.0" prefWidth="151.0" stylesheets="@../../styles2.css" text="Cancel">
               <HBox.margin>
                  <Insets right="175.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="saveButton" onAction="#onSave" prefHeight="40.0" prefWidth="150.0" style="-fx-background-color: #ffdb00; -fx-text-fill: #222222;" stylesheets="@../../styles.css" text="Save">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
      </HBox>
    </VBox>
</AnchorPane>
