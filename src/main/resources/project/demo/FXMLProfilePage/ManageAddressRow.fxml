<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="121.0" prefWidth="552.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.demo.controllers.Profile.ManageAddressRowController">
   <children>
      <VBox layoutX="5.0" layoutY="7.0" prefHeight="108.0" prefWidth="552.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="106.4" prefWidth="552.0" spacing="15.0" style="-fx-border-color: #e0e0e0; -fx-border-width: 0 0 1 0; -fx-padding: 15;">
               <children>
                  <VBox alignment="CENTER_LEFT" prefWidth="500.0" spacing="5.0">
                     <children>
                        <Label fx:id="AddressTyepeLabel" prefHeight="26.0" prefWidth="422.0" style="-fx-font-weight: bold; -fx-font-size: 16px;" text="Adress Name" />
                        <Label fx:id="StreetCityPostalCodeLabel" prefHeight="20.0" prefWidth="533.0" style="-fx-font-size: 14px; -fx-text-fill: #666666;" text="Street, City,  Postal Code" />
                        <Label fx:id="ProvinceRegionLabel" prefHeight="20.0" prefWidth="403.0" style="-fx-font-size: 14px; -fx-text-fill: #666666;" text=" Province, Region" />
                     </children>
                  </VBox>
                  <HBox spacing="10.0" />
                  <Button mnemonicParsing="false" onAction="#onEditClicked" prefHeight="32.0" prefWidth="23.0" style="-fx-background-color: F4F4F5;">
                     <graphic>
                        <ImageView fitHeight="33.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../imagelogo/pen.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#onDeleteClicked" prefHeight="32.0" prefWidth="23.0" style="-fx-background-color: wF4F4F5hite;">
                     <graphic>
                        <ImageView fitHeight="33.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../imagelogo/bin.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
