<AnchorPane prefHeight="115.0" prefWidth="595.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.demo.controllers.Profile.ManageAddressRowController">
   <children>
      <Pane layoutX="10.0" layoutY="11.0" prefHeight="92.0" prefWidth="576.0" style="-fx-background-color: #f5f5f5; -fx-background-radius: 5; -fx-border-color: #dcdcdc; -fx-border-radius: 5;">
         <children>
            <VBox layoutX="9.0" layoutY="5.0" prefHeight="74.0" prefWidth="558.0">
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="79.0" prefWidth="558.0" spacing="15.0">
                     <children>
                        <VBox alignment="CENTER_LEFT" prefHeight="66.0" prefWidth="406.0" spacing="5.0">
                           <children>
                              <Label fx:id="AddressTyepeLabel" prefHeight="26.0" prefWidth="422.0" style="-fx-font-weight: bold; -fx-font-size: 14px;" text="Adress Name" />
                              <Label fx:id="StreetCityPostalCodeLabel" prefHeight="18.0" prefWidth="366.0" text="Street, City,  Postal Code" />
                              <Label fx:id="ProvinceRegionLabel" prefHeight="20.0" prefWidth="403.0" text=" Province, Region" />
