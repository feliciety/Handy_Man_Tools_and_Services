UPDATE service
SET service_price = REPLACE(service_price, '$', '')
WHERE service_price LIKE '$%';


CREATE TABLE employees (
    employee_id INT AUTO_INCREMENT PRIMARY KEY,
    employee_image VARCHAR(255) NOT NULL,
    employee_name VARCHAR(255) NOT NULL,
    service_id INT NOT NULL,
    availability VARCHAR(50) DEFAULT 'Available',
    CONSTRAINT fk_service FOREIGN KEY (service_id) REFERENCES service(service_id) ON DELETE CASCADE ON UPDATE CASCADE);

